# OpenClaw Gateway Configuration
# Copy this file to .env and fill in your values

# WebSocket URL for OpenClaw Gateway
# Use ws://127.0.0.1:18789 for local development
# Use wss://your-gateway-url for remote/production
OPENCLAW_GATEWAY_URL=ws://127.0.0.1:18789

# Authentication token for OpenClaw Gateway
# Get this from /root/.openclaw/openclaw.json -> gateway.auth.token
OPENCLAW_GATEWAY_TOKEN=your_token_here

# For client-side usage (Next.js)
NEXT_PUBLIC_OPENCLAW_GATEWAY_URL=ws://127.0.0.1:18789
NEXT_PUBLIC_OPENCLAW_GATEWAY_TOKEN=your_token_here
